<?xml version="1.0" encoding="UTF-8"?>
<permissionPackage display="User Permissions" orderNo="1">
	<!-- key and group display used by java and html start-->
	<permissionGroup display="View Jobs">
		<permission key="resellerportal:view_job" display="Enabled"/>
	</permissionGroup>
	<permissionGroup display="Create/Edit Job">
		<permission key="resellerportal:create_edit_job" display="Enabled" dependsOn="resellerportal:view_job"/>
	</permissionGroup>
	<!-- key used by java and html end-->
	<permissionGroup display="Create/Edit Users">
		<permission key="resellerportal:create_edit_user" display="Enabled"/>
	</permissionGroup>	
	<permissionGroup display="Download Updates">
		<permission key="resellerportal:download_update" display="Enabled"/>
	</permissionGroup>
	<!-- <permissionGroup display="Download Beta Updates">
		<permission key="resellerportal:download_beta_update" display="Enabled"/>
	</permissionGroup> -->
	<permissionGroup display="Request Suppliers">
		<permission key="resellerportal:request_supplier" display="Enabled"/>
	</permissionGroup>
</permissionPackage>